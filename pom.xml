              </reportSet>
              <reportSet>
                <id>aggregate</id>
                <reports>
                  <report>aggregate-no-fork</report>
                  <report>test-aggregate-no-fork</report>
                </reports>
                <inherited>false</inherited>
              </reportSet>
            </reportSets>
          </plugin>
          <plugin>
            <artifactId>maven-jxr-plugin</artifactId>
            <configuration>
              <aggregate>true</aggregate>
            </configuration>
          </plugin>

					<xsd:enumeration value="islensk"/>
					<xsd:enumeration value="given-only"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attributeGroup ref="language.atts"/>
	</xsd:attributeGroup>

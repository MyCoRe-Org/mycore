				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:attributeGroup>
	<xsd:attributeGroup name="contributor.atts">
		<xsd:attribute name="sequence" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">

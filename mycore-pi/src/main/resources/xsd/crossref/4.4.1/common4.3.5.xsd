						<xsd:enumeration value="TG"/>
						<xsd:enumeration value="TK"/>
						<xsd:enumeration value="TO"/>
						<xsd:enumeration value="TT"/>
						<xsd:enumeration value="TN"/>
						<xsd:enumeration value="TR"/>
						<xsd:enumeration value="TM"/>

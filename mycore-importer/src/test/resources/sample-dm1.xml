<?xml version="1.0" encoding="utf-8"?>
<configuration type="sample-dm1">
    <structure />
    <metadata>

        <element name="def.metaText" minOccurs='1' maxOccurs='1'>
            <mcrmetaxml name="metaText" class="MCRMetaLangText" minOccurs='1' maxOccurs='1' length="1024" />
        </element>

        <element name="def.metaXML" maxOccurs='1'>
            <mcrmetaxml name="metaXML" class="MCRMetaXML" minOccurs='1' maxOccurs='unbounded' length="256" />
        </element>

        <element name="dates" minOccurs='0' maxOccurs='1'>
            <mcrmetaiso8601date name="date" class="MCRMetaISO8601Date" minOccurs='1' maxOccurs='1' />
        </element>

        <element name="def.link" minOccurs='0' maxOccurs='1'>
            <mcrmetalinkid name="link" class="MCRMetaLinkID" minOccurs='0' maxOccurs='unbounded' />
        </element>

    </metadata>
</configuration>